{
  "name": "Oceanic",
  "version": "1.0.0",
  "description": "A deep blue theme inspired by the ocean.",
  "author": "DesQTA Community",
  "license": "MIT",
  "compatibility": {
    "minVersion": "1.0.0",
    "maxVersion": "2.0.0"
  },
  "preview": {
    "thumbnail": "/external-themes/oceanic/preview.png",
    "screenshots": []
  },
  "settings": {
    "defaultAccentColor": "#0077be",
    "defaultTheme": "dark",
    "supportsLightMode": true,
    "supportsDarkMode": true,
    "supportsSystemMode": true
  },
  "customProperties": {
    "primaryColor": "#0077be",
    "secondaryColor": "#00bfff",
    "successColor": "#20b2aa",
    "warningColor": "#ffd700",
    "errorColor": "#ff4500",
    "backgroundColor": "#0a192f",
    "surfaceColor": "#112240",
    "textColor": "#e0f7fa",
    "borderColor": "#0077be",
    "shadowColor": "rgba(0, 119, 190, 0.2)"
  },
  "fonts": {
    "primary": "Inter",
    "secondary": "Arial",
    "monospace": "JetBrains Mono"
  },
  "animations": {
    "duration": "200ms",
    "easing": "cubic-bezier(0.4, 0, 0.2, 1)",
    "enableEnhanced": true
  },
  "features": {
    "customScrollbars": true,
    "glassmorphism": false,
    "gradients": true,
    "shadows": true
  }
} 